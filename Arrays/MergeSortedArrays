const mergeSorted = (nums1, m, nums2, n) => {
  let p1 = 0;
  let p2 = 0;
  for (let i = 0; i < m + n; i++) {
    if ((p1 < m && nums1[p1] < nums2[p2]) || p2 >= n) {
      nums1[i] = nums1[p1];
      p1++;
    } else {
      nums1[i] = nums2[p2];
      p2++;
    }
  }

  return nums1;
};

console.log(mergeSorted([1, 2, 3, 0, 0, 0], 3, [4, 8, 19], 3));
